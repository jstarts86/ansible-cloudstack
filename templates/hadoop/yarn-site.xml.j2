<configuration>
    <property>
        <name>yarn.resourcemanager.hostname</name>
        <value>{{ namenode_hostname }}</value>
    </property>
    <property>
        <name>yarn.resourcemanager.webapp.address</name>
        <value>Master1:8088</value>
    </property>
    <property>
        <name>yarn.nodemanager.aux-services</name>
        <value>mapreduce_shuffle</value>
    </property>
    <property>
        <name>yarn.acl.enable</name>
        <value>{{ yarn_acl_enable }}</value>
    </property>
    <property>
        <name>yarn.nodemanager.resource.memory-mb</name>
        <value>{{ yarn_nodemanager_resource_memory_mb }}</value>
    </property>
    <property>
        <name>yarn.scheduler.maximum-allocation-mb</name>
        <value>{{ yarn_scheduler_maximum_allocation_mb }}</value>
    </property>
    <property>
        <name>yarn.scheduler.minimum-allocation-mb</name>
        <value>{{ yarn_scheduler_minimum_allocation_mb }}</value>
    </property>
    <property>
        <name>yarn.nodemanager.vmem-check-enabled</name>
        <value>{{ yarn_nodemanager_vmem_check_enabled }}</value>
    </property>
</configuration>
